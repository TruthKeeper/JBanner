<resources>

    <declare-styleable name="JBanner">
        <!--页数最大上限，默认10-->
        <attr name="pagerLimit" format="integer" />
        <!--自动播放，默认true-->
        <attr name="autoPlay" format="boolean" />
        <!--轮播时间间隔(ms)，默认5_000ms -->
        <attr name="intervalTime" format="integer" />
        <!--翻页速度(ms)，默认600ms -->
        <attr name="fadeTime" format="integer" />
        <!--是否禁止触摸滑动-->
        <attr name="disableScroll" format="boolean" />
    </declare-styleable>
</resources>
